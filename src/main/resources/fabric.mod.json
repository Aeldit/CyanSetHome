{
  "schemaVersion": 1,
  "id": "cyansethome",
  "version": "${version}",
  "name": "CyanSetHome",
  "description": "This mod adds the /sethome command and everything that comes with it",
  "authors": [
    "Aeldit"
  ],
  "contact": {
    "homepage": "https://modrinth.com/user/Aeldit",
    "sources": "https://github.com/Aeldit/CyanSetHome",
    "issues": "https://github.com/Aeldit/CyanSetHome/issues"
  },
  "license": "GPL-3.0 license",
  "icon": "assets/cyansh/icon.png",
  "environment": "*",
  "entrypoints": {
    "server": [
      "fr.aeldit.cyansethome.CyanSHServerCore"
    ],
    "client": [
      "fr.aeldit.cyansethome.CyanSHClientCore"
    ],
    "modmenu": [
      "fr.aeldit.cyansethome.config.ModMenuApiImpl"
    ]
  },
  "depends": {
    "minecraft": ">=${min} <=${max}",
    "java": ">=${java_version}",
    "fabricloader": ">=${loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "cyanlib": "=${cyanlib_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/PcYPpqzhKS"
      }
    }
  }
}
